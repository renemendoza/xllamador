<h1>Login</h1>
<% semantic_form_for @user_session do |form|%>
  <%= form.error_messages %>

  <% form.inputs do %>
    <%= form.input :username, :label => 'Username' %>
    <%= form.input :password, :label => 'Password' %>
  <% end %>
  <% form.buttons do %>
    <%= form.commit_button "Log In" %>
  <% end %>
<% end -%>

